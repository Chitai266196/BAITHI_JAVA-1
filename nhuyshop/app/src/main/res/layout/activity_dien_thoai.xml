<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:id="@+id/Layout_Dienthoai"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nguyennhuy.nhuyshop.activity.DienThoaiActivity">

    <android.support.v7.widget.Toolbar
        android:background="#716562"
        android:titleTextColor="#f9f6f5"
        app:title="Điện thoại"
        android:id="@+id/toolbardienthoai"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"></android.support.v7.widget.Toolbar>


    <ListView
        android:id="@+id/listviewdienthoai"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></ListView>
</LinearLayout>
